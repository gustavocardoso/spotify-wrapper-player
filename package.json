{
  "name": "gus-spotify-wrapper-player",
  "version": "1.0.1",
  "description": "A simple Spotify player using the Spotify Wrapper",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "./node_modules/.bin/mocha --require ./tests/testHelper.js tests/**/*.spec.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "nyc npm test",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "start": "./node_modules/.bin/webpack-dev-server"
  },
  "files": [
    "dist",
    "lib"
  ],
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "keywords": [
    "js",
    "javascript",
    "tdd",
    "tests"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gustavocardoso/spotify-wrapper-player"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge"
  },
  "homepage": "https://github.com/gustavocardoso/spotify-wrapper-player#readme",
  "author": "Gustavo Cardoso",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.0",
    "node-fetch": "^3.1.1",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "gus-spotify-wrapper": "^1.0.2"
  }
}
